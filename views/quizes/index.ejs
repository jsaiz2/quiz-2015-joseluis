<!--/* formulario para las búsquedas */ -->

<form method="get" action="/quizes/busqueda">
	<p>
	<input type="text" name="search" value="Escriba su búsqueda aquí  "/>
	<input type="submit" value="Buscar"/></p>
</form>
<table>
<% var i; for (i=0; i < quizes.length; i++) { %>
	<tr><td><a href="quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td></tr>
	<% } %>
</table>